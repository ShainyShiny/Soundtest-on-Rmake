{
    "name": "Soundtest on Rmake",
    "description": "公開されている音楽・効果音素材を試聴できる簡易プレイヤー",
    "version": "1.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://rmake.jp/published_items/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}